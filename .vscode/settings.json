{
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "window.zoomLevel": 0.1,
    "liveServer.settings.port": 5501,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/__init__.py": false,
        "**/test_venv": true
    },
    "explorerExclude.backup": {},
    "workbench.colorCustomizations": {
    "sideBar.foreground": "#d4d4d4",          // цвет текста в боковом меню
    "sideBarSectionHeader.background": "#333333", // цвет заголовков разделов
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarTitle.foreground": "#ffffff",     // цвет заголовка бокового меню
    "sideBar.border": "#252526"                // цвет рамки бокового меню
}

}